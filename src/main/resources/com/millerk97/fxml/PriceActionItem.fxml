    <Label fx:id="anomalyThresholdLabel" GridPane.columnIndex="1" GridPane.rowIndex="11"/>
    <Label text="Current Influenceability Score" GridPane.rowIndex="12"/>
    <Label fx:id="isLabel" GridPane.columnIndex="1" GridPane.rowIndex="12"/>
    <Label text="Magnitude (in Multiple)" GridPane.rowIndex="13"/>
    <Label fx:id="magnitudeLabel" GridPane.columnIndex="1" GridPane.rowIndex="13"/>
    <Label text="Magnitude of Bitcoin" GridPane.rowIndex="14"/>
    <Label fx:id="bitcoinMagnitudeLabel" GridPane.columnIndex="1" GridPane.rowIndex="14"/>
    <Label text="Mag attr. to external Factors" GridPane.rowIndex="15"/>
    <HBox alignment="CENTER_LEFT" spacing="20.0" GridPane.columnIndex="1" GridPane.rowIndex="15">
        <GridPane.margin>
            <Insets bottom="10.0" top="10.0"/>
        </GridPane.margin>
        <Label fx:id="magExternalLabel"/>
        <Button fx:id="showTweetsButton" mnemonicParsing="false" text="Show tweets"/>
    </HBox>
</fx:root>